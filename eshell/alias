alias fnd find . -name $*
alias gr grep -rnw './' -e $*
alias la ls -a $*
alias ll ls -l $*
alias lla ls -la $*
alias pop popd $*
alias push pushd ./ $*
alias up cd .. $*
alias ff find-file $1
alias ssh cd /ssh:$1:~/
alias file file -h $1
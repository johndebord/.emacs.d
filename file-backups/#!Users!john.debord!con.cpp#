#include <eosio/multi_index.hpp>

using namespace eosio;

struct [[eosio::contract]] con : eosio::contract {
   using contract::contract;

   struct [[eosio::table]] structure {
      std::vector<int> vec;
   };

   [[eosio::action]] void act()
   { }
};


#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

#include "bencode.h"

int main()
{
    clock_t c_start = clock(); // START CLOCK

    const char* str = "d9:publisher3:bob17:publisher-webpage15:www.example.com18:publisher.location4:homee";
    be_node* node = be_decoden(str, strlen(str));
    // be_dump(node);
    // be_free(node);

    clock_t c_end = clock(); // END CLOCK
    float time_elapsed_ms = 1000.0 * (c_end-c_start) / CLOCKS_PER_SEC; // CALC CLOCK
    printf("CPU time used: %f ms\n", time_elapsed_ms);
    
    return 0;
}

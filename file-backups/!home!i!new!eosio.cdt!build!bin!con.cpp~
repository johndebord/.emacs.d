#include <eosio/eosio.hpp>
using namespace eosio;

CONTRACT hello : public contract {
   public:
      using contract::contract;

      ACTION hi( name nm ) {
          print_f("Name : %\n", nm);
      }
      
      ACTION check( name nm ){
          print_f("Name : %\n", nm);
          eosio::check(nm == "hello"_n, "check name not equal to `hello`");
      }

      
};
